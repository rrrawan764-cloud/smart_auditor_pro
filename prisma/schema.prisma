datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Case {
  id        String   @id @default(uuid())
  title     String
  facts     String
  analysis  String?
  status    String   @default("PENDING")
  createdAt DateTime @default(now())
}
